SELECT nome, telefone -- inserir os campos da tabela
AS Pessoas -- novo nome temporario
FROM Pessoal -- nome da tabela atual

MIN = Valor Mínimo
MAX = Valor Máximo
AVG = Média Aritmética
SUM = Total(Soma)
COUNT = Contar a quantidade de itens

SELECT MAX(id_pessoal) FROM Pessoal;
SELECT MIN(id_pessoal) FROM Pessoal;
SELECT AVG(id_pessoal) FROM Pessoal;
SELECT SUM(id_pessoal) FROM Pessoal;
☝️ Não pode dar espaço

UPDATE Pessoal
SET cpf = "002" WHERE id_pessoal = 2;
